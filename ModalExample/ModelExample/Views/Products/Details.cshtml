@model ModelExample.Models.Product

@{
    ViewBag.Title = "Details";
}

<h2>Product Details</h2>

<table class="table">
    <tr>
        <th>Product Id</th>
        @*<td>@ViewBag.MatchingProduct.ProductId</td>*@
        <td>@Model.ProductId</td>
    </tr>
    <tr>
        <th>Product Name</th>
        @*<td>@ViewBag.MatchingProduct.ProductName</td>*@
        <td>@Model.ProductName</td>
    </tr>
    <tr>
        <th>Rate</th>
        @*<td>@ViewBag.MatchingProduct.Rate</td>*@
        <td>@Model.Rate</td>
    </tr>
</table>