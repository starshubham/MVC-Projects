
<html>
<head>
    <title>Index</title>
</head>
<body>
    <h4>CORS</h4>
    <input type="button" id="button1" value="Get data from server" />
    <table border="1" id="table1">
        <tr>
            <th>Emp ID</th>
            <th>Emp Name</th>
            <th>Emp Salary</th>
        </tr>
    </table>

    <script src="~/Scripts/jquery-3.7.0.js"></script>
    <script>
        $("#button1").click(function () {
            $.ajax({
                url: "https://localhost:44393/api/employees",
                type: "GET",
                success: fun1,
                error: fun2
            });
        });

        function fun1(response) {
            for (var i = 0; i < response.length; i++) {
                $("#table1").append("<tr><td>" + response[i].EmpID + "</td><td>" +
                    response[i].EmpName + "</td><td>" + response[i].Salary + "</td></tr>");
            }
        }

        function fun2() {
            console.log("error");
        }
    </script>
</body>
</html>
